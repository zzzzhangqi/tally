<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.record">


    <application
        android:name=".action.UniteApp"
        android:allowBackup="true"
        android:icon="@mipmap/log12"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tally">
        <activity
            android:name=".action.login.ForgetActivity"
            android:exported="false" />
        <activity
            android:name=".action.login.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".action.login.LoginActivity"
            android:exported="false" />
        <activity android:name=".action.MonthChartActivity" />
        <activity android:name=".action.SettingActivity" />
        <activity android:name=".action.HistoryActivity" />
        <activity android:name=".action.AboutActivity" />
        <activity android:name=".action.SearchActivity" />
        <activity android:name=".action.RecordActivity" />
        <activity android:name=".action.MainActivity" />
        <activity android:name=".action.ShakeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>